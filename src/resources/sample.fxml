<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="820.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <top>
        <ToolBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="40.0" prefWidth="800.0" style="-fx-background-color: #5ea1bc;" BorderPane.alignment="CENTER">
            <items>
                <Button fx:id="backbtn" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #f49242;" text="&lt;-" />
                <Button fx:id="nextbtn" mnemonicParsing="false" onAction="#next" style="-fx-background-color: f49242;" text="-&gt;" />
                <TextField fx:id="addressBar" onAction="#onEnter" prefHeight="25.0" prefWidth="383.0" promptText="Enter URL here..." />
                <Button fx:id="gobtn" mnemonicParsing="false" onAction="#go" style="-fx-background-color: #bfe896;" text="GO" />
            </items>
        </ToolBar>
    </top>
    <center>
        <WebView fx:id="web" minHeight="400.0" minWidth="800.0" prefHeight="400.0" BorderPane.alignment="CENTER" />
    </center>
</BorderPane>
